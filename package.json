{
  "name": "discord-bot-api",
  "version": "1.8.0",
  "main": "index.js",
  "license": "MIT",
  "author": {
    "name": "Rafael Tessarollo"
  },
  "scripts": {
    "prepare": "husky install",
    "start": "tsx src/main/server.ts",
    "dev": "tsx watch src/main/server.ts",
    "build": "rimraf dist && tsc -p tsconfig-build.json",
    "debug": "tsx watch --inspect=0.0.0.0:9222 --nolazy src/main/server.ts",
    "up": "yarn build && docker-compose up -d",
    "down": "docker-compose down",
    "test": "vitest --threads=false --single-thread --sequence.shuffle --sequence.seed 1698238251980 --run --passWithNoTests --silent",
    "test:staged": "yarn test related",
    "test:ci": "yarn test --coverage --retry=1"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^18.6.3",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/validator": "^13.7.5",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@vitest/coverage-istanbul": "^0.34.6",
    "coveralls": "^3.1.1",
    "eslint": "^8.0.1",
    "eslint-config-standard-with-typescript": "^22.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "git-commit-msg-linter": "^4.1.3",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "mockdate": "^3.0.5",
    "rimraf": "^3.0.2",
    "supertest": "^6.3.3",
    "tsx": "^3.14.0",
    "typescript": "^4.7.4",
    "vitest": "^0.34.6",
    "vitest-mongodb": "^0.0.5"
  },
  "dependencies": {
    "amqplib": "^0.10.3",
    "axios": "^1.1.3",
    "bcrypt": "^5.0.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.1",
    "jsonwebtoken": "^9.0.0",
    "module-alias": "^2.2.2",
    "mongodb": "^4.8.1",
    "socket.io": "^4.6.1",
    "socket.io-client": "^4.6.1",
    "swagger-ui-express": "^4.5.0",
    "validator": "^13.7.0"
  },
  "engines": {
    "node": ">=16.x"
  },
  "_moduleAliases": {
    "@": "dist"
  }
}
